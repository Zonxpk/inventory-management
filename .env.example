# Database connection URL (used by the backend)
# PRISMA_DATABASE_URL takes precedence over POSTGRES_URL
PRISMA_DATABASE_URL=postgresql://user:password@localhost:5432/inventory

# Fallback database connection URL
POSTGRES_URL=postgresql://user:password@localhost:5432/inventory

# Backend API base URL (used by the frontend, defaults to http://localhost:3001)
NEXT_PUBLIC_INVENTORY_API_URL=http://localhost:3001
